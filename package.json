  "scripts": {
    "start": "npm-run-all -p start:build start:run",
    "clean": "rm dist/bundle.js",
    "build:dev": "webpack -d --mode development",
    "build:prod": "webpack -p --mode production",
    "start:build": "nodemon --config nodemon.build.json --exec \"node --stack-trace-limit=1000 -- ./node_modules/webpack/bin/webpack.js --mode development --watch\"",
    "start:run": "nodemon --config nodemon.run.json --exec \"node --stack-trace-limit=1000 -- ./dist/bundle.js\""
  },
  "dependencies": {
    "colors": "^1.4.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.0"
  }
}
